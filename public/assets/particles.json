{
  "fpsLimit": 30,
  "particles": {
    "number": {
      "value": 0
    },
    "color": {
      "value": [
        "#0f0"
      ]
    },
    "shadow": {
      "blur": 50,
      "color": {
        "value": "#000000"
      },
      "enable": true,
      "offset": {
        "x": 1,
        "y": 1
      }
    },
    "shape": {
      "type": "square"
    },
    "opacity": {
      "value": 1
    },
    "size": {
      "value": 2,
      "random": {
        "enable": true,
        "minimumValue": 1
      }
    },
    "move": {
      "enable": true,
      "speed": 2,
      "direction": "top",
      "outMode": "destroy"
    }
  },
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "resize": true
    }
  },
  "detectRetina": true,

  "emitters": {
    "direction": "top",
    "position": {
      "x": 50,
      "y": 100
    },
    "rate": {
      "delay": 0.2,
      "quantity": 1
    },
    "size": {
      "width": 100,
      "height": 0
    }
  }
}